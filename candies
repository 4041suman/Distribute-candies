class Solution {
    public int distributeCandies(int[] candyType) {
        Set<Integer> set = new HashSet<>();
        int count=candyType.length/2;
        
        for(int i=0;i<candyType.length;i++){
            set.add(candyType[i]);
            }
        int a=Math.min(set.size(),count);
       return a;
    }
}
